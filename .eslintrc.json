{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-use-before-define": 0,
        "no-unused-vars": 0,
        "no-undef": 0,
        "no-param-reassign": 0
    }
}
